public class Box {
    private double width;
    private double height;
    private double depth;

    
    public Box() {
        this.width = 1.0;
        this.height = 1.0;
        this.depth = 1.0;
    }

   
    public double getVolume() {
        return width * height * depth;
    }

    public static void main(String[] args) {
        Box box = new Box();
        System.out.println("Volume of the box: " + box.getVolume());
    }
}
